    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'baffao-core'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=baffao-core"
                ],
                "filter": {
                    "name": "baffao-core",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'baffao-proxy'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=baffao-proxy",
                    "--package=baffao-proxy"
                ],
                "filter": {
                    "name": "baffao-proxy",
                    "kind": "bin"
                }
            },
            "env": {
                "RUST_BACKTRACE": "full"
            },
            "args": [],
            "cwd": "${workspaceFolder}/baffao-proxy"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'baffao-proxy'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=baffao-proxy",
                    "--package=baffao-proxy"
                ],
                "filter": {
                    "name": "baffao-proxy",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}